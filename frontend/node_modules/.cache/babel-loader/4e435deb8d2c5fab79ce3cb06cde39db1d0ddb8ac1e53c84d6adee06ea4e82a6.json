{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, reactive, computed, onMounted, watch } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { message } from 'ant-design-vue';\nimport softwareAssetApi from '@/api/softwareAsset';\nimport dayjs from 'dayjs';\n\n// 路由实例\n\nexport default {\n  __name: 'SoftwareAssetForm',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const route = useRoute();\n\n    // 表单引用\n    const formRef = ref();\n\n    // 是否为编辑模式\n    const isEdit = computed(() => !!route.params.id);\n\n    // 提交状态\n    const submitting = ref(false);\n\n    // 表单数据\n    const formData = reactive({\n      software_name: '',\n      version: '',\n      vendor: '',\n      software_type: '',\n      purchase_date: null,\n      purchase_cost: null,\n      license_type: '',\n      license_count: 1,\n      used_count: 0,\n      license_start_date: null,\n      license_end_date: null,\n      license_status: 'valid',\n      license_key: '',\n      deployment_location: '',\n      responsible_person: '',\n      contact_info: '',\n      notes: ''\n    });\n\n    // 表单验证规则\n    const formRules = {\n      software_name: [{\n        required: true,\n        message: '请输入软件名称',\n        trigger: 'blur'\n      }],\n      version: [{\n        required: true,\n        message: '请输入版本号',\n        trigger: 'blur'\n      }],\n      vendor: [{\n        required: true,\n        message: '请输入供应商',\n        trigger: 'blur'\n      }],\n      software_type: [{\n        required: true,\n        message: '请选择软件类型',\n        trigger: 'change'\n      }],\n      license_type: [{\n        required: true,\n        message: '请选择许可证类型',\n        trigger: 'change'\n      }],\n      license_count: [{\n        required: true,\n        message: '请输入许可证数量',\n        trigger: 'blur'\n      }, {\n        type: 'number',\n        min: 1,\n        message: '许可证数量必须大于0',\n        trigger: 'blur'\n      }],\n      used_count: [{\n        type: 'number',\n        min: 0,\n        message: '已使用数量不能小于0',\n        trigger: 'blur'\n      }, {\n        validator: (rule, value) => {\n          if (value > formData.license_count) {\n            return Promise.reject('已使用数量不能大于许可证总数');\n          }\n          return Promise.resolve();\n        },\n        trigger: 'blur'\n      }],\n      license_start_date: [{\n        required: true,\n        message: '请选择许可证开始日期',\n        trigger: 'change'\n      }],\n      license_end_date: [{\n        validator: (rule, value) => {\n          if (!value) {\n            return Promise.resolve();\n          }\n          if (formData.license_start_date && dayjs(value).isBefore(dayjs(formData.license_start_date))) {\n            return Promise.reject('到期日期不能早于开始日期');\n          }\n          return Promise.resolve();\n        },\n        trigger: 'change'\n      }],\n      responsible_person: [{\n        required: true,\n        message: '请输入负责人',\n        trigger: 'blur'\n      }]\n    };\n\n    // 监听许可证数量变化，自动调整已使用数量\n    watch(() => formData.license_count, newCount => {\n      if (formData.used_count > newCount) {\n        formData.used_count = newCount;\n      }\n    });\n\n    // 加载数据\n    const loadData = async () => {\n      if (!isEdit.value) return;\n      try {\n        const response = await softwareAssetApi.getDetail(route.params.id);\n        const data = response.data;\n\n        // 填充表单数据\n        Object.keys(formData).forEach(key => {\n          if (data[key] !== undefined) {\n            if (key.includes('date') && data[key]) {\n              formData[key] = dayjs(data[key]);\n            } else {\n              formData[key] = data[key];\n            }\n          }\n        });\n      } catch (error) {\n        message.error('加载数据失败：' + error.message);\n        router.back();\n      }\n    };\n\n    // 处理提交\n    const handleSubmit = async () => {\n      try {\n        await formRef.value.validate();\n        submitting.value = true;\n\n        // 准备提交数据\n        const submitData = {\n          ...formData\n        };\n\n        // 转换日期格式\n        if (submitData.purchase_date) {\n          submitData.purchase_date = submitData.purchase_date.format('YYYY-MM-DD');\n        }\n        if (submitData.license_start_date) {\n          submitData.license_start_date = submitData.license_start_date.format('YYYY-MM-DD');\n        }\n        if (submitData.license_end_date) {\n          submitData.license_end_date = submitData.license_end_date.format('YYYY-MM-DD');\n        }\n        if (isEdit.value) {\n          await softwareAssetApi.update(route.params.id, submitData);\n          message.success('更新成功');\n        } else {\n          await softwareAssetApi.create(submitData);\n          message.success('创建成功');\n        }\n        router.push('/assets/software');\n      } catch (error) {\n        if (error.errorFields) {\n          message.error('请检查表单填写是否正确');\n        } else {\n          message.error((isEdit.value ? '更新' : '创建') + '失败：' + error.message);\n        }\n      } finally {\n        submitting.value = false;\n      }\n    };\n\n    // 处理取消\n    const handleCancel = () => {\n      router.back();\n    };\n\n    // 生命周期\n    onMounted(() => {\n      loadData();\n    });\n    const __returned__ = {\n      router,\n      route,\n      formRef,\n      isEdit,\n      submitting,\n      formData,\n      formRules,\n      loadData,\n      handleSubmit,\n      handleCancel,\n      ref,\n      reactive,\n      computed,\n      onMounted,\n      watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get message() {\n        return message;\n      },\n      get softwareAssetApi() {\n        return softwareAssetApi;\n      },\n      get dayjs() {\n        return dayjs;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","onMounted","watch","useRouter","useRoute","message","softwareAssetApi","dayjs","router","route","formRef","isEdit","params","id","submitting","formData","software_name","version","vendor","software_type","purchase_date","purchase_cost","license_type","license_count","used_count","license_start_date","license_end_date","license_status","license_key","deployment_location","responsible_person","contact_info","notes","formRules","required","trigger","type","min","validator","rule","value","Promise","reject","resolve","isBefore","newCount","loadData","response","getDetail","data","Object","keys","forEach","key","undefined","includes","error","back","handleSubmit","validate","submitData","format","update","success","create","push","errorFields","handleCancel"],"sources":["E:/A_project/yuanchi/AIops/frontend/src/views/business/SoftwareAssetForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"software-asset-form-container\">\r\n    <div class=\"form-header\">\r\n      <h2>{{ isEdit ? '编辑软件资产' : '新增软件资产' }}</h2>\r\n      <div class=\"header-actions\">\r\n        <a-button @click=\"handleCancel\">取消</a-button>\r\n        <a-button type=\"primary\" @click=\"handleSubmit\" :loading=\"submitting\">\r\n          {{ isEdit ? '更新' : '保存' }}\r\n        </a-button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"form-content\">\r\n      <a-form\r\n        ref=\"formRef\"\r\n        :model=\"formData\"\r\n        :rules=\"formRules\"\r\n        layout=\"vertical\"\r\n        :label-col=\"{ span: 24 }\"\r\n        :wrapper-col=\"{ span: 24 }\"\r\n      >\r\n        <a-row :gutter=\"24\">\r\n          <!-- 基本信息 -->\r\n          <a-col :span=\"24\">\r\n            <div class=\"form-section\">\r\n              <h3>基本信息</h3>\r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"软件名称\" name=\"software_name\">\r\n                    <a-input v-model:value=\"formData.software_name\" placeholder=\"请输入软件名称\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"版本\" name=\"version\">\r\n                    <a-input v-model:value=\"formData.version\" placeholder=\"请输入版本号\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"供应商\" name=\"vendor\">\r\n                    <a-input v-model:value=\"formData.vendor\" placeholder=\"请输入供应商\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n              \r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"软件类型\" name=\"software_type\">\r\n                    <a-select v-model:value=\"formData.software_type\" placeholder=\"请选择软件类型\">\r\n                      <a-select-option value=\"system\">系统软件</a-select-option>\r\n                      <a-select-option value=\"application\">应用软件</a-select-option>\r\n                      <a-select-option value=\"development\">开发工具</a-select-option>\r\n                      <a-select-option value=\"security\">安全软件</a-select-option>\r\n                      <a-select-option value=\"database\">数据库</a-select-option>\r\n                      <a-select-option value=\"middleware\">中间件</a-select-option>\r\n                      <a-select-option value=\"other\">其他</a-select-option>\r\n                    </a-select>\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"采购日期\" name=\"purchase_date\">\r\n                    <a-date-picker\r\n                      v-model:value=\"formData.purchase_date\"\r\n                      placeholder=\"请选择采购日期\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"采购成本\" name=\"purchase_cost\">\r\n                    <a-input-number\r\n                      v-model:value=\"formData.purchase_cost\"\r\n                      placeholder=\"请输入采购成本\"\r\n                      :min=\"0\"\r\n                      :precision=\"2\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n            </div>\r\n          </a-col>\r\n\r\n          <!-- 许可证信息 -->\r\n          <a-col :span=\"24\">\r\n            <div class=\"form-section\">\r\n              <h3>许可证信息</h3>\r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"许可证类型\" name=\"license_type\">\r\n                    <a-select v-model:value=\"formData.license_type\" placeholder=\"请选择许可证类型\">\r\n                      <a-select-option value=\"perpetual\">永久许可</a-select-option>\r\n                      <a-select-option value=\"subscription\">订阅许可</a-select-option>\r\n                      <a-select-option value=\"trial\">试用许可</a-select-option>\r\n                      <a-select-option value=\"free\">免费许可</a-select-option>\r\n                      <a-select-option value=\"oem\">OEM许可</a-select-option>\r\n                    </a-select>\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"许可证数量\" name=\"license_count\">\r\n                    <a-input-number\r\n                      v-model:value=\"formData.license_count\"\r\n                      placeholder=\"请输入许可证数量\"\r\n                      :min=\"1\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"已使用数量\" name=\"used_count\">\r\n                    <a-input-number\r\n                      v-model:value=\"formData.used_count\"\r\n                      placeholder=\"请输入已使用数量\"\r\n                      :min=\"0\"\r\n                      :max=\"formData.license_count\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n              \r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"许可证开始日期\" name=\"license_start_date\">\r\n                    <a-date-picker\r\n                      v-model:value=\"formData.license_start_date\"\r\n                      placeholder=\"请选择开始日期\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"许可证到期日期\" name=\"license_end_date\">\r\n                    <a-date-picker\r\n                      v-model:value=\"formData.license_end_date\"\r\n                      placeholder=\"请选择到期日期\"\r\n                      style=\"width: 100%\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"许可证状态\" name=\"license_status\">\r\n                    <a-select v-model:value=\"formData.license_status\" placeholder=\"请选择许可证状态\">\r\n                      <a-select-option value=\"valid\">有效</a-select-option>\r\n                      <a-select-option value=\"expired\">已过期</a-select-option>\r\n                      <a-select-option value=\"near_expired\">即将过期</a-select-option>\r\n                      <a-select-option value=\"unlimited\">永久</a-select-option>\r\n                    </a-select>\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n              \r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"24\">\r\n                  <a-form-item label=\"许可证密钥\" name=\"license_key\">\r\n                    <a-textarea\r\n                      v-model:value=\"formData.license_key\"\r\n                      placeholder=\"请输入许可证密钥\"\r\n                      :rows=\"3\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n            </div>\r\n          </a-col>\r\n\r\n          <!-- 部署信息 -->\r\n          <a-col :span=\"24\">\r\n            <div class=\"form-section\">\r\n              <h3>部署信息</h3>\r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"部署位置\" name=\"deployment_location\">\r\n                    <a-input v-model:value=\"formData.deployment_location\" placeholder=\"请输入部署位置\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"负责人\" name=\"responsible_person\">\r\n                    <a-input v-model:value=\"formData.responsible_person\" placeholder=\"请输入负责人\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n                <a-col :span=\"8\">\r\n                  <a-form-item label=\"联系方式\" name=\"contact_info\">\r\n                    <a-input v-model:value=\"formData.contact_info\" placeholder=\"请输入联系方式\" />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n            </div>\r\n          </a-col>\r\n\r\n          <!-- 其他信息 -->\r\n          <a-col :span=\"24\">\r\n            <div class=\"form-section\">\r\n              <h3>其他信息</h3>\r\n              <a-row :gutter=\"16\">\r\n                <a-col :span=\"24\">\r\n                  <a-form-item label=\"备注\" name=\"notes\">\r\n                    <a-textarea\r\n                      v-model:value=\"formData.notes\"\r\n                      placeholder=\"请输入备注信息\"\r\n                      :rows=\"4\"\r\n                    />\r\n                  </a-form-item>\r\n                </a-col>\r\n              </a-row>\r\n            </div>\r\n          </a-col>\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, computed, onMounted, watch } from 'vue';\r\nimport { useRouter, useRoute } from 'vue-router';\r\nimport { message } from 'ant-design-vue';\r\nimport softwareAssetApi from '@/api/softwareAsset';\r\nimport dayjs from 'dayjs';\r\n\r\n// 路由实例\r\nconst router = useRouter();\r\nconst route = useRoute();\r\n\r\n// 表单引用\r\nconst formRef = ref();\r\n\r\n// 是否为编辑模式\r\nconst isEdit = computed(() => !!route.params.id);\r\n\r\n// 提交状态\r\nconst submitting = ref(false);\r\n\r\n// 表单数据\r\nconst formData = reactive({\r\n  software_name: '',\r\n  version: '',\r\n  vendor: '',\r\n  software_type: '',\r\n  purchase_date: null,\r\n  purchase_cost: null,\r\n  license_type: '',\r\n  license_count: 1,\r\n  used_count: 0,\r\n  license_start_date: null,\r\n  license_end_date: null,\r\n  license_status: 'valid',\r\n  license_key: '',\r\n  deployment_location: '',\r\n  responsible_person: '',\r\n  contact_info: '',\r\n  notes: ''\r\n});\r\n\r\n// 表单验证规则\r\nconst formRules = {\r\n  software_name: [\r\n    { required: true, message: '请输入软件名称', trigger: 'blur' }\r\n  ],\r\n  version: [\r\n    { required: true, message: '请输入版本号', trigger: 'blur' }\r\n  ],\r\n  vendor: [\r\n    { required: true, message: '请输入供应商', trigger: 'blur' }\r\n  ],\r\n  software_type: [\r\n    { required: true, message: '请选择软件类型', trigger: 'change' }\r\n  ],\r\n  license_type: [\r\n    { required: true, message: '请选择许可证类型', trigger: 'change' }\r\n  ],\r\n  license_count: [\r\n    { required: true, message: '请输入许可证数量', trigger: 'blur' },\r\n    { type: 'number', min: 1, message: '许可证数量必须大于0', trigger: 'blur' }\r\n  ],\r\n  used_count: [\r\n    { type: 'number', min: 0, message: '已使用数量不能小于0', trigger: 'blur' },\r\n    {\r\n      validator: (rule, value) => {\r\n        if (value > formData.license_count) {\r\n          return Promise.reject('已使用数量不能大于许可证总数');\r\n        }\r\n        return Promise.resolve();\r\n      },\r\n      trigger: 'blur'\r\n    }\r\n  ],\r\n  license_start_date: [\r\n    { required: true, message: '请选择许可证开始日期', trigger: 'change' }\r\n  ],\r\n  license_end_date: [\r\n    {\r\n      validator: (rule, value) => {\r\n        if (!value) {\r\n          return Promise.resolve();\r\n        }\r\n        if (formData.license_start_date && dayjs(value).isBefore(dayjs(formData.license_start_date))) {\r\n          return Promise.reject('到期日期不能早于开始日期');\r\n        }\r\n        return Promise.resolve();\r\n      },\r\n      trigger: 'change'\r\n    }\r\n  ],\r\n  responsible_person: [\r\n    { required: true, message: '请输入负责人', trigger: 'blur' }\r\n  ]\r\n};\r\n\r\n// 监听许可证数量变化，自动调整已使用数量\r\nwatch(() => formData.license_count, (newCount) => {\r\n  if (formData.used_count > newCount) {\r\n    formData.used_count = newCount;\r\n  }\r\n});\r\n\r\n// 加载数据\r\nconst loadData = async () => {\r\n  if (!isEdit.value) return;\r\n  \r\n  try {\r\n    const response = await softwareAssetApi.getDetail(route.params.id);\r\n    const data = response.data;\r\n    \r\n    // 填充表单数据\r\n    Object.keys(formData).forEach(key => {\r\n      if (data[key] !== undefined) {\r\n        if (key.includes('date') && data[key]) {\r\n          formData[key] = dayjs(data[key]);\r\n        } else {\r\n          formData[key] = data[key];\r\n        }\r\n      }\r\n    });\r\n  } catch (error) {\r\n    message.error('加载数据失败：' + error.message);\r\n    router.back();\r\n  }\r\n};\r\n\r\n// 处理提交\r\nconst handleSubmit = async () => {\r\n  try {\r\n    await formRef.value.validate();\r\n    \r\n    submitting.value = true;\r\n    \r\n    // 准备提交数据\r\n    const submitData = { ...formData };\r\n    \r\n    // 转换日期格式\r\n    if (submitData.purchase_date) {\r\n      submitData.purchase_date = submitData.purchase_date.format('YYYY-MM-DD');\r\n    }\r\n    if (submitData.license_start_date) {\r\n      submitData.license_start_date = submitData.license_start_date.format('YYYY-MM-DD');\r\n    }\r\n    if (submitData.license_end_date) {\r\n      submitData.license_end_date = submitData.license_end_date.format('YYYY-MM-DD');\r\n    }\r\n    \r\n    if (isEdit.value) {\r\n      await softwareAssetApi.update(route.params.id, submitData);\r\n      message.success('更新成功');\r\n    } else {\r\n      await softwareAssetApi.create(submitData);\r\n      message.success('创建成功');\r\n    }\r\n    \r\n    router.push('/assets/software');\r\n  } catch (error) {\r\n    if (error.errorFields) {\r\n      message.error('请检查表单填写是否正确');\r\n    } else {\r\n      message.error((isEdit.value ? '更新' : '创建') + '失败：' + error.message);\r\n    }\r\n  } finally {\r\n    submitting.value = false;\r\n  }\r\n};\r\n\r\n// 处理取消\r\nconst handleCancel = () => {\r\n  router.back();\r\n};\r\n\r\n// 生命周期\r\nonMounted(() => {\r\n  loadData();\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.software-asset-form-container {\r\n  padding: 24px;\r\n  background: #fff;\r\n  min-height: 100vh;\r\n}\r\n\r\n.form-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 24px;\r\n  padding-bottom: 16px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.form-header h2 {\r\n  margin: 0;\r\n  font-size: 20px;\r\n  font-weight: 600;\r\n  color: #262626;\r\n}\r\n\r\n.header-actions {\r\n  display: flex;\r\n  gap: 8px;\r\n}\r\n\r\n.form-content {\r\n  max-width: 1200px;\r\n}\r\n\r\n.form-section {\r\n  margin-bottom: 32px;\r\n  padding: 24px;\r\n  background: #fafafa;\r\n  border-radius: 8px;\r\n  border: 1px solid #f0f0f0;\r\n}\r\n\r\n.form-section h3 {\r\n  margin: 0 0 24px 0;\r\n  font-size: 16px;\r\n  font-wei